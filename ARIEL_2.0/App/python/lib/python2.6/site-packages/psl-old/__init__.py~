
print "init PSL 1.0"

import os

versions = {}
versions[("Darwin",'10.4.0')] = "osx-10.6"
versions[("Darwin",'10.7.0')] = "osx-10.6"
versions[("Darwin",'10.8.0')] = "osx-10.6"
versions[("Darwin",'9.8.0')] = "osx-10.5"

version = versions[(os.uname()[0],os.uname()[2])]

print "version is",version

import sys
import string

sys.path.append(string.join(string.split(__file__,"/")[:-1],"/")+"/platform/"+version)

d = os.getcwd()

print "using path",sys.path[-1]

os.chdir(sys.path[-1])

import openframeworks
print openframeworks

from openframeworks import *

os.chdir(d)

